@model List<EntityLayer.Concrete.Ship>
@{
    ViewData["Title"] = "Ship List";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<div class="float-left mb-3">
    <h3>Ship List</h3>
</div>
<div class="float-right">
    <a href="~/Ship/Create">
        <input type="button" value="Add" class="btn btn-info" />
    </a>
</div>


<table class="table table-sm table-hover table-responsive w-100 d-block d-md-table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Ship Id</th>
            <th>Ship Name</th>
            <th>Ship Type</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.ShipId</td>
                <td>@item.ShipName</td>
                <td>@item.ShipType</td>
            </tr>
        }
    </tbody>
</table>